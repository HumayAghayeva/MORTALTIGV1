<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Solona Tiger</title>
    <!-- Bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />

    <!-- Main css -->
    <link rel="stylesheet" href="/css/style.css" />

    <!-- Media css -->
    <link rel="stylesheet" href="/css/media.css" />

    <!-- Slick css -->
    <link rel="stylesheet" type="text/css" href="/plugins/slick/slick.css" />
    <link rel="stylesheet" type="text/css" href="/plugins/slick/slick-theme.css" />

    <!-- Scroll animation -->
    <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300&display=swap" rel="stylesheet" />
</head>
<body>
    <header id="header">
        <div class="container">
            <div class="row" id="header">
                <div class="col-lg-2 col-8 header-item logo-wrapper">
                    <a href="#" class="logo"><img src="/images/logo_thumbnail.png" alt=""></a>
                </div>
                <div class="col-lg-8 col-4 header-item navigation-wrapper">
                    <div class="hamburger-menu-wrapper">
                        <img src="/images/hamburger.png" alt="" />
                    </div>
                    <nav id="nav">
                        <ul>
                            <li><a href="#header">home</a></li>
                            <li><a href="#nft">nft</a></li>
                            <li><a href="#team">team</a></li>
                            <li><a href="#roadmap">roadmap</a></li>
                            <li><a href="#faq">faq</a></li>
                            <li><a href="#staking.html">staking</a></li>
                            <li><a href="#partners">partners</a></li>
                        </ul>
                    </nav>
                </div>
                <div class="col-lg-2 header-item connection-button-wrapper">
                    <a href="#" class="connect-button solona-button">connect wallet</a>
                </div>
            </div>
        </div>
    </header>
    <div class="">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <!-- Boostrap -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous">
    </script>



    <!-- Html End -->

    <script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
    <script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
    <script type="text/javascript" src="plugins/slick/slick.min.js"></script>

    <!-- Main js -->
    <script src="js/script.js"></script>
        <script>
        $(".nft-wrapper").slick({
            accessibility: true,
            dots: true,
            centerMode: true,
            centerPadding: "0px",
            slidesToShow: 3,
            slidesToScroll: 1,
            // autoplay: true,
            // autoplaySpeed: 1000,
            responsive: [{
                breakpoint: 1200,
                settings: {
                    arrows: false,
                    centerMode: true,
                    centerPadding: "0px",
                    slidesToShow: 2,
                },
            },
            {
                breakpoint: 992,
                settings: {
                    arrows: false,
                    centerMode: true,
                    centerPadding: "0px",
                    slidesToShow: 2,
                },
            },
            {
                breakpoint: 768,
                settings: {
                    arrows: false,
                    centerMode: true,
                    centerPadding: "0px",
                    slidesToShow: 1,
                },
            },
            {
                breakpoint: 480,
                settings: {
                    arrows: false,
                    centerMode: true,
                    centerPadding: "0px",
                    slidesToShow: 1,
                },
            },
            ],
        });
    </script>

    <!-- Scroll animation -->
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script>
        AOS.init();
    </script>
</body>
</html>
